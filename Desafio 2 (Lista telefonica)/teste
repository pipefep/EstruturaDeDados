https://pythonhelp.wordpress.com/2011/12/10/filter-filtrando-elementos-de-uma-lista/

iniFim = True

while(iniFim):

    i = 0
    print(i)
    vetA = [9]
    vetB = [9]

    print("--- Lista telefonica ---\n")
    print("Digite uma operação")
    print("1. Inserir um novo telefone.")
    print("2. Buscar um determinado telefone.")
    print("3. Remover um determinado telefone")

    esc = int(input('Escolha: '))

    if esc == 1:
        vetA[i] = input("Digite o Número: ")


    elif esc == 2:
        proc = input("Digite o Número para ser buscado: ")

        if ((proc in vetB) == True):
            print("O número {} EXISTE na lista telefonica!".format(proc))
        else:
            print("O número {} NÃO EXISTE na lista telefonica!".format(proc))


    i += 1
